<!--pages/register/register.wxml-->
<!-- register.wxml -->
<view class="weui-cells weui-cells_after-title">
  <view class="weui-cell weui-cell_input">
    <view class="weui-cell__hd">
      <view class="weui-label">姓名</view>
    </view>
    <view class="weui-cell__bd">
      <input class="weui-input" placeholder='填写姓名' bindchange='changeName'> 填写姓名
      </input>
    </view>
  </view>
  <view class="weui-cell weui-cell_input">
    <view class="weui-cell__hd">
      <view class="weui-label">手机号</view>
    </view>
    <view class="weui-cell__bd">
      <input class="weui-input" placeholder='填写手机号' bindchange='changeTel'> 填写手机号
      </input>
    </view>
  </view>
  <view class="weui-cell weui-cell_input">
    <view class="weui-cell__hd">
      <view class="weui-label">学校</view>
    </view>
    <view class="weui-cell__bd">
      <input class="weui-input" placeholder='填写学校' bindchange='changeSchool'> 填写学校
      </input>
    </view>
  </view>
  <view class="weui-cell weui-cell_input">
    <view class="weui-cell__hd">
      <view class="weui-label">学号</view>
    </view>
    <view class="weui-cell__bd">
      <input class="weui-input" placeholder='填写学号' bindchange='changeNum'> 填写学号
      </input>
    </view>
  </view>
  <view class="weui-cell weui-cell_input">
    <view class="weui-cell__hd">
      <view class="weui-label">入学年份</view>
    </view>
    <view class="weui-cell__bd">
      <input class="weui-input" placeholder='填写入学年份如2017' bindchange='changeYear'> 填写入学年份
      </input>
    </view>
  </view>
</view>
<view class="page__bd page__bd_spacing submit">
  <button class="weui-btn" type="primary" bindtap="bindSubmit">提交</button>
</view>
