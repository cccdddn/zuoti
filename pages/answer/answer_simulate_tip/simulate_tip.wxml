<!--index.wxml-->
<view class="simulate-tx">
    <image  mode="aspectFill" src="{{userInfo.avatarUrl}}"></image>
</view>
<view class="simulate-name">{{userInfo.nickName}}</view>
<view class="simulate-info">
    <view> 考试数量：100题</view>
    <view> 考试时间：45分钟</view>
    <view> 合格标准：满分100分，90分及格</view>
    <view> 出题规则：按公安规定比例随机抽取</view>
</view>
<navigator url="../../pages/answer_simulate_info/simulate_info?subject={{subject}}&type={{type}}" redirect class="simulate-btn">开始考试</navigator>